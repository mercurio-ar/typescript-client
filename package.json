{
  "name": "@mercurio-ar/typescript-client",
  "version": "1.0.0",
  "description": "typescript client for mercurio api",
  "main": "src/index.ts",
  "scripts": {
    "lint": "tslint --fix ./src/**/*",
    "test": "jest --no-cache"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test -- --coverage"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testRegex": "/src/.*\\.test\\.(ts|tsx)$",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.(ts|tsx)$": "typescript-babel-jest"
    }
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.6.1",
    "husky": "^1.0.1",
    "jest": "^23.6.0",
    "jshint": "^2.9.6",
    "lint-staged": "^7.3.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript-babel-jest": "^1.0.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mercurio-ar/typescript-client.git"
  },
  "author": "Sebastian Gonzalez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mercurio-ar/typescript-client/issues"
  },
  "homepage": "https://github.com/mercurio-ar/typescript-client#readme",
  "dependencies": {
    "@mercurio-ar/model": "file:../model",
    "axios": "^0.18.0",
    "querystring": "^0.2.0",
    "typescript": "^3.1.1"
  }
}
